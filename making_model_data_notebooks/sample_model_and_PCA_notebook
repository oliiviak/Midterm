  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "#average arr_delay by carrier and by quarter\n",
    "#ONLY pass test_sample\n",
    "def avg_delay_carrier_quarter(df):\n",
    "\n",
    "    avg_delay_by_carrier = df.groupby(['mkt_unique_carrier', 'quarter'])[['arr_delay']].mean().copy().reset_index()\n",
    "\n",
    "    unique_carrier = df['mkt_unique_carrier'].unique()\n",
    "    unique_quarter = df['quarter'].unique()\n",
    "\n",
    "    for c in unique_carrier:\n",
    "        for n in unique_quarter:\n",
    "            try:\n",
    "                value = float(avg_delay_by_carrier.loc[(avg_delay_by_carrier['mkt_unique_carrier'] == c) & (avg_delay_by_carrier['quarter'] == n)]['arr_delay'])\n",
    "            except:\n",
    "                value = np.nan\n",
    "            df.loc[(df['mkt_unique_carrier'] == c) & (df['quarter'] == n), 'avg_delay_carrier'] = value\n",
    "            \n",
    "    return df"
   ]
  },
