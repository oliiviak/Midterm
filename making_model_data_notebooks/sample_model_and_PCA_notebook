    "kk_test_sample = traffic_by_origin_airport(jj_test_sample)\n",
    "kk_test_sample #37columns"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [],
   "source": [
    "#traffic by destination airport \n",
    "#ONLY pass test_sample\n",
    "def traffic_by_dest_airport(df):\n",
    "\n",
    "    dest_airport_traffic = df.groupby(['dest_airport_id', 'time_period_arr'])['fl_date'].count().copy().reset_index()\n",
    "\n",
    "    unique_airport_id = df['dest_airport_id'].unique()\n",
    "    unique_time_period = df['time_period_arr'].unique()\n",
    "\n",
    "    for a in unique_airport_id:\n",
    "        for t in unique_time_period:\n",
    "            try:\n",
    "                value = float(dest_airport_traffic.loc[(dest_airport_traffic['dest_airport_id'] == a) & (dest_airport_traffic['time_period_arr'] == t)]['fl_date'])\n",
    "                df.loc[(df['dest_airport_id'] == a) & (df['time_period_arr'] == t), 'traffic_dest_port'] = value\n",
    "            except:\n",
    "                value = np.nan\n",
    "    return df\n"
   ]
  },
