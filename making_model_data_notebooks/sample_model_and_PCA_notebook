  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "metadata": {},
   "outputs": [],
   "source": [
    "#average weather condition by airport and time period \n",
    "#ONLY on test_sample\n",
    "def weather_by_airport_period(df):\n",
    "\n",
    "    weather_og_airport_period = df.groupby(['origin_airport_id', 'time_period_dep'])['origin_weather'].mean().copy().reset_index()\n",
    "\n",
    "    unique_og_airport = df['origin_airport_id'].unique()\n",
    "    unique_period = df['time_period_dep'].unique()\n",
    "\n",
    "    for a in unique_og_airport:\n",
    "        for p in unique_period:\n",
    "            try:\n",
    "                value = float(weather_og_airport_period.loc[(weather_og_airport_period['origin_airport_id'] == a) & (weather_og_airport_period['time_period_dep'] == p)]['origin_weather'])\n",
    "                df.loc[(df['origin_airport_id'] == a) & (df['time_period_dep'] == p), 'weather_og_period'] = value\n",
    "            except:\n",
    "                value = np.nan\n",
    "\n",
    "\n",
    "    weather_dest_airport_period = df.groupby(['dest_airport_id', 'time_period_arr'])['dest_weather'].mean().copy().reset_index()\n",
    "\n",
    "    unique_dest_airport = df['dest_airport_id'].unique()\n",
    "    unique_period = df['time_period_arr'].unique()\n",
    "\n",
    "    for a in unique_dest_airport:\n",
    "        for p in unique_period:\n",
    "            try:\n",
    "                value = float(weather_dest_airport_period.loc[(weather_dest_airport_period['dest_airport_id'] == a) & (weather_dest_airport_period['time_period_arr'] == p)]['dest_weather'])\n",
    "                df.loc[(df['dest_airport_id'] == a) & (df['time_period_arr'] == p), 'weather_dest_period'] = value\n",
    "            except:\n",
    "                value = np.nan\n",
    "    return df\n"
   ]
  },
