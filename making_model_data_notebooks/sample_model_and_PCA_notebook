  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "def convert_date_time(n):\n",
    "    if len(n) == 1:\n",
    "        n = '000' + n\n",
    "        return n \n",
    "    elif len(n) == 2:\n",
    "        n = '00' + n\n",
    "        return n \n",
    "    elif len(n) == 3:\n",
    "        n = '0' + n\n",
    "        return n \n",
    "    else:\n",
    "        return n \n",
    "\n",
    "#apply to test_sample \n",
    "def make_hour_only(df):\n",
    "    df['crs_dep_time'] = df['crs_dep_time'].astype(str)\n",
    "    df['crs_arr_time'] = df['crs_arr_time'].astype(str)\n",
    "    \n",
    "    df['hour_only_dep'] = df['crs_dep_time'].apply(lambda x: convert_date_time(x))\n",
    "    df['hour_only_dep'] = df['hour_only_dep'].str[:2]\n",
    "    \n",
    "    df['hour_only_arr'] = df['crs_arr_time'].apply(lambda x: convert_date_time(x))\n",
    "    df['hour_only_arr'] = df['hour_only_arr'].str[:2]\n",
    "\n",
    "    return df "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [
    {
     "output_type": "execute_result",
     "data": {
      "text/plain": [
       "          fl_date    origin_city_name  origin_airport_id origin  \\\n",
