  {
   "cell_type": "code",
   "execution_count": 35,
   "metadata": {},
   "outputs": [],
   "source": [
    "#encoding of distance and indication of delay\n",
    "#apply to test_sample \n",
    "def distance_filter_and_is_delayed(df):\n",
    "    \n",
    "    def distance(x):\n",
    "        if x >= 3000:\n",
    "            return 2 #long\n",
    "        elif x < 3000 and x >= 700:\n",
    "            return 1 #medium\n",
    "        elif x < 700:\n",
    "            return 0 #short\n",
    "    \n",
    "    df[\"distance_encoded\"] = df[\"distance\"].apply(distance)\n",
    "      \n",
    "    def is_delayed(x):\n",
    "        if x > 0:\n",
    "            return 1\n",
    "        elif x <= 0:\n",
    "            return 0\n",
    "    \n",
    "    df[\"is_delayed\"] = df[\"arr_delay\"].apply(is_delayed)\n",
    "\n",
    "    return df"
   ]
  },
