   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [],
   "source": [
    "#traffic by origin airport and depature time period \n",
    "#ONLY pass test_sample\n",
    "def traffic_by_origin_airport(df):\n",
    "\n",
    "    origin_airport_traffic = df.groupby(['origin_airport_id', 'time_period_dep'])['fl_date'].count().copy().reset_index()\n",
    "\n",
    "    unique_airport_id = df['origin_airport_id'].unique()\n",
    "    unique_time_period = df['time_period_dep'].unique()\n",
    "\n",
    "    for a in unique_airport_id:\n",
    "        for t in unique_time_period:\n",
    "            try:\n",
    "                value = float(origin_airport_traffic.loc[(origin_airport_traffic['origin_airport_id'] == a) & (origin_airport_traffic['time_period_dep'] == t)]['fl_date'])\n",
    "                df.loc[(df['origin_airport_id'] == a) & (df['time_period_dep'] == t), 'traffic_origin_port'] = value\n",
    "            except:\n",
    "                value = np.nan\n",
    "    return df\n"
   ]
  },
