  {
   "cell_type": "code",
   "execution_count": 41,
   "metadata": {},
   "outputs": [],
   "source": [
    "#make the origin_weather, dest_weather enumerable \n",
    "oo_test_sample['origin_weather'] = oo_test_sample['origin_weather'].replace('Unknown', np.nan)\n",
    "oo_test_sample = oo_test_sample.dropna()\n",
    "oo_test_sample['origin_weather'] = oo_test_sample['origin_weather'].astype(int)\n",
    "\n",
    "oo_test_sample['dest_weather'] = oo_test_sample['dest_weather'].replace('Unknown', np.nan)\n",
    "oo_test_sample = oo_test_sample.dropna()    \n",
    "oo_test_sample['dest_weather'] = oo_test_sample['dest_weather'].astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "metadata": {},
   "outputs": [],
   "source": [
    "#average weather condition by origin airport and quarter\n",
    "#ONLY on test_sample\n",
    "def weather_by_airport_quarter(df):\n",
    "\n",
    "    weather_og_airport_quarter = df.groupby(['origin_airport_id', 'quarter'])['origin_weather'].mean().copy().reset_index()\n",
    "\n",
    "    unique_og_airport = df['origin_airport_id'].unique()\n",
    "    unique_quarter = df['quarter'].unique()\n",
    "\n",
    "    for a in unique_og_airport:\n",
    "        for q in unique_quarter:\n",
    "            try:\n",
    "                value = float(weather_og_airport_quarter.loc[(weather_og_airport_quarter['origin_airport_id'] == a) & (weather_og_airport_quarter['quarter'] == q)]['origin_weather'])\n",
    "                df.loc[(df['origin_airport_id'] == a) & (df['quarter'] == q), 'weather_og_quarter'] = value\n",
    "            except:\n",
    "                value = np.nan\n",
    "\n",
    "\n",
    "    weather_dest_airport_quarter = df.groupby(['dest_airport_id', 'quarter'])['dest_weather'].mean().copy().reset_index()\n",
    "\n",
    "    unique_dest_airport = df['dest_airport_id'].unique()\n",
    "    unique_quarter = df['quarter'].unique()\n",
    "\n",
    "    for a in unique_dest_airport:\n",
    "        for q in unique_quarter:\n",
    "            try:\n",
    "                value = float(weather_dest_airport_quarter.loc[(weather_dest_airport_quarter['dest_airport_id'] == a) & (weather_dest_airport_quarter['quarter'] == q)]['dest_weather'])\n",
    "                df.loc[(df['dest_airport_id'] == a) & (df['quarter'] == q), 'weather_dest_quarter'] = value\n",
    "            except:\n",
    "                value = np.nan\n",
    "    return df\n"
   ]
  },
