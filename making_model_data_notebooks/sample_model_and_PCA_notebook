  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "#average delay by weather condition \n",
    "#ONLY on test_sample\n",
    "def delay_by_weather(df):\n",
    "\n",
    "    og_weather_delay = df.groupby('origin_weather')['arr_delay'].mean().copy().reset_index()\n",
    "\n",
    "    unique_og_weather = df['origin_weather'].unique()\n",
    "    for w in unique_og_weather:\n",
    "        try:\n",
    "            value = float(og_weather_delay.loc[(og_weather_delay['origin_weather'] == w)]['arr_delay'])\n",
    "            df.loc[(df['origin_weather'] == w), 'weather_og_delay'] = value\n",
    "        except:\n",
    "            value = np.nan\n",
    "\n",
    "\n",
    "    dest_weather_delay = df.groupby('dest_weather')['arr_delay'].mean().copy().reset_index()\n",
    "\n",
    "    unique_dest_weather = df['dest_weather'].unique()\n",
    "    for w in unique_dest_weather:\n",
    "        try:\n",
    "            value = float(dest_weather_delay.loc[(dest_weather_delay['dest_weather'] == w)]['arr_delay'])\n",
    "            df.loc[(df['dest_weather'] == w), 'weather_dest_delay'] = value\n",
    "        except:\n",
    "            value = np.nan\n",
    "\n",
    "    return df\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
