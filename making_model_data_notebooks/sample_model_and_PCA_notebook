   "source": [
    "import datetime as dt\n",
    "import pandas as pd\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "#use on df_test_sample\n",
    "def separate_datetime(df):\n",
    "\n",
    "    df['fl_date']= pd.to_datetime(df['fl_date'])\n",
    "    df['quarter'] = df['fl_date'].dt.quarter\n",
    "    df['year'] = df['fl_date'].dt.year\n",
    "    df['month'] = df['fl_date'].dt.month\n",
    "    df['day'] = df['fl_date'].dt.day\n",
    "\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
