  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "#transform the dep, arr times into a time period \n",
    "#ONLY pass test_sample\n",
    "def get_time_period_(df):\n",
    "    df['hour_only_dep'] = df['hour_only_dep'].astype(int)\n",
    "    df['hour_only_arr'] = df['hour_only_arr'].astype(int)\n",
    "\n",
    "    for hd in df['hour_only_dep']:\n",
    "        if 0 <= hd <= 5:\n",
    "            df.loc[(df['hour_only_dep'] == hd), 'time_period_dep'] = 0\n",
    "        elif 6 <= hd <= 11:\n",
    "            df.loc[(df['hour_only_dep'] == hd), 'time_period_dep'] = 1\n",
    "        elif 12 <= hd <= 17:\n",
    "            df.loc[(df['hour_only_dep'] == hd), 'time_period_dep'] = 2\n",
    "        else:\n",
    "            df.loc[(df['hour_only_dep'] == hd), 'time_period_dep'] = 3\n",
    "\n",
    "    for ha in df['hour_only_arr']:\n",
    "        if 0 <= ha <= 5:\n",
    "            df.loc[(df['hour_only_arr'] == ha), 'time_period_arr'] = 0\n",
    "        elif 6 <= ha <= 11:\n",
    "            df.loc[(df['hour_only_arr'] == ha), 'time_period_arr'] = 1\n",
    "        elif 12 <= ha <= 17:\n",
    "            df.loc[(df['hour_only_arr'] == ha), 'time_period_arr'] = 2\n",
    "        else:\n",
    "            df.loc[(df['hour_only_arr'] == ha), 'time_period_arr'] = 3\n",
    "    return df\n"
   ]
  },
