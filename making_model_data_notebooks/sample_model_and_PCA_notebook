   "outputs": [],
   "source": [
    "#traffic by time period \n",
    "#ONLY on test_sample\n",
    "def traffic_by_time_period(df):\n",
    "\n",
    "    dep_time_period_traffic = df.groupby(['time_period_dep'])['arr_delay'].count().copy().reset_index()\n",
    "\n",
    "    unique_time_period = df['time_period_dep'].unique()\n",
    "\n",
    "    for t in unique_time_period:\n",
    "        try:\n",
    "            value = float(dep_time_period_traffic.loc[(dep_time_period_traffic['time_period_dep'] == t)]['arr_delay'])\n",
    "            df.loc[(df['time_period_dep'] == t), 'dep_time_traffic'] = value\n",
    "        except:\n",
    "            value = np.nan\n",
    "\n",
    "\n",
    "    arr_time_period_traffic = df.groupby(['time_period_arr'])['arr_delay'].count().copy().reset_index()\n",
    "\n",
    "    unique_time_period = df['time_period_arr'].unique()\n",
    "\n",
    "    for t in unique_time_period:\n",
    "        try:\n",
    "            value = float(arr_time_period_traffic.loc[(arr_time_period_traffic['time_period_arr'] == t)]['arr_delay'])\n",
    "            df.loc[(df['time_period_arr'] == t), 'arr_time_traffic'] = value\n",
    "        except:\n",
    "            value = np.nan\n",
    "    return df\n"
   ]
  },
