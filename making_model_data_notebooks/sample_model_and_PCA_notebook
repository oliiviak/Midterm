   "cell_type": "code",
   "execution_count": 37,
   "metadata": {},
   "outputs": [],
   "source": [
    "#average delay by encoded distances \n",
    "#ONLY on test_sample\n",
    "def avg_delay_by_distance(df):\n",
    "\n",
    "    avg_distance_delay = df.groupby(['distance_encoded'])['arr_delay'].mean().copy().reset_index()\n",
    "\n",
    "    unique_distance_encoding = df['distance_encoded'].unique()\n",
    "\n",
    "    for d in unique_distance_encoding:\n",
    "        try:\n",
    "            value = float(avg_distance_delay.loc[(avg_distance_delay['distance_encoded'] == d)]['arr_delay'])\n",
    "            df.loc[(df['distance_encoded'] == d), 'avg_delay_distance'] = value\n",
    "        except:\n",
    "            value = np.nan\n",
    "    return df\n"
   ]
  },
