   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "#average delay by carrier, destination airport, and quarter\n",
    "#ONLY PASS test_sample\n",
    "def avg_delay_dest_airport_quarter(df):\n",
    "\n",
    "    avg_delay_by_port_quart = df.groupby(['mkt_unique_carrier', 'dest_airport_id', 'quarter'])[['arr_delay']].mean().copy().reset_index()\n",
    "\n",
    "    unique_carrier = df['mkt_unique_carrier'].unique()\n",
    "    unique_quarter = df['quarter'].unique()\n",
    "    unique_airport_id = df['dest_airport_id'].unique()\n",
    "\n",
    "    for c in unique_carrier:\n",
    "        for n in unique_quarter:\n",
    "            for a in unique_airport_id:\n",
    "                try:\n",
    "                    value = float(avg_delay_by_port_quart.loc[(avg_delay_by_port_quart['mkt_unique_carrier'] == c) & (avg_delay_by_port_quart['quarter'] == n) & (avg_delay_by_port_quart['dest_airport_id'] == a)]['arr_delay'])\n",
    "                except:\n",
    "                    value = np.nan\n",
    "                df.loc[(df['mkt_unique_carrier'] == c) & (df['quarter'] == n) & (df['dest_airport_id'] == a), 'avg_delay_dest_quart'] = value\n",
    "    \n",
    "    return df\n"
   ]
  },
